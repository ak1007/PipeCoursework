    <assert component="T1" invert="true" method="isSelected" />
    <action args="T1,(25%2c21),±90°" class="pipe.dataLayer.Transition" method="actionSelectPopupMenuItem" />
    <action args="SelectionObject Instance,(457%2c75)" class="pipe.gui.SelectionObject" method="actionClick" />
    <action args="SelectionObject Instance,(479%2c123)" method="actionClick" />
    <assert component="T1" method="isSelected" />
    <action args="SelectionObject Instance,(366%2c66)" class="pipe.gui.SelectionObject" method="actionClick" />
    <!--Now change the label of T1-->
    <action args="T1,(20%2c23),Edit label" class="pipe.dataLayer.Transition" method="actionSelectPopupMenuItem" />
    <wait args="Input" class="abbot.tester.ComponentTester" method="assertComponentShowing" />
    <action args="JTextField Instance,wake up" method="actionKeyString" />
    <action args="OK" class="javax.swing.AbstractButton" method="actionClick" />
    <wait args="Input" class="abbot.tester.ComponentTester" invert="true" method="assertComponentShowing" />
    <assert args="wake up" class="abbot.tester.ComponentTester" method="assertComponentShowing" />
    <assert component="wake up" method="getText" value="wake up" />
    <!--Then use popup to "disable timing" for T1/wakeup and check that it is now solid black-->
    <action args="T1,(21%2c12),Disable Timing" class="pipe.dataLayer.Transition" method="actionSelectPopupMenuItem" />
    <action args="SelectionObject Instance,(260%2c76)" method="actionClick" />
