    <action args="T1,(19%2c19)" class="pipe.dataLayer.Transition" method="actionClick" />
    <action args="P2,(19%2c20)" class="pipe.dataLayer.Place" method="actionClick" />
    <assert args="Arc Instance" class="abbot.tester.ComponentTester" method="assertComponentShowing" />
    <assert args="Arc Instance 2" class="abbot.tester.ComponentTester" method="assertComponentShowing" />
    <assert args="Arc Instance 3" class="abbot.tester.ComponentTester" method="assertComponentShowing" />
    <assert args="Arc Instance 4" class="abbot.tester.ComponentTester" method="assertComponentShowing" />
    <!--Next, start adding an arc, and then hit Escape. Confirm arc not created.-->
    <action args="P2" method="actionClick" />
    <action args="GuiView Instance,(466%2c236),BUTTON1_MASK|SHIFT_MASK" class="pipe.gui.GuiView" method="actionClick" />
    <action args="GuiView Instance,(375%2c123)" class="pipe.gui.GuiView" method="actionClick" />
    <action args="GuiView Instance,(206%2c203)" class="pipe.gui.GuiView" method="actionClick" />
