  <!-- These are the definitions of the specific components that the script drives -->
  <!-- Now the actions begin -->
  <launch args="[]" class="RunGui" classpath="" method="main" />
  <sequence>
    <!-- Check that "Select" button is depressed at startup -->
    <assert component="Images%5cSelect.png" method="isEnabled" />
    <assert component="Images%5cSelect.png" method="isSelected" />
    <!-- Click Place option on the Draw menu and check that Place button reflects change -->
    <action args="Place" method="actionSelectMenuItem" />
    <assert component="Images%5cPlace.png" method="isSelected" />
    <!-- Click three times on drawing area and check we now have three places -->
