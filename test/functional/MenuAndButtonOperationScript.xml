    <action args="GuiView Instance,(472%2c119)" class="pipe.gui.GuiView" method="actionClick" />
    <assert args="T1" class="abbot.tester.ComponentTester" method="assertComponentShowing" />
    <!-- Click on places and transitions and confirm we have four arcs -->
