    <action args="P0" class="abbot.tester.AbstractButtonTester" method="actionClick" />
    <action args="P0" class="abbot.tester.AbstractButtonTester" method="actionClick" />
    <call args="P0 should have 2 tokens,P0,2" class="pipe.gui.FunctionalScriptsTest" method="assertNumberOfTokensInPlace" />
