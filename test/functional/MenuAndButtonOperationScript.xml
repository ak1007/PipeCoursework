    <action args="JTextField Instance,VK_ENTER" method="actionKeyStroke" />
    <wait args="Input" class="abbot.tester.ComponentTester" invert="true" method="assertComponentShowing" />
    <assert args="2" class="abbot.tester.ComponentTester" method="assertComponentShowing" />
    <!--Use context menu to add a token to P1-->
    <action args="P1,(19%2c24),Edit Tokens" class="pipe.dataLayer.Place" method="actionSelectPopupMenuItem" />
    <wait args="Input" class="abbot.tester.ComponentTester" method="assertComponentShowing" />
    <action args="JTextField Instance,VK_BACK_SPACE" method="actionKeyStroke" />
    <action args="JTextField Instance,1" method="actionKeyString" />
    <action args="JTextField Instance,VK_ENTER" method="actionKeyStroke" />
    <call args="P1 incorrect tokens after popup menu edit,P1,1" class="pipe.gui.FunctionalScriptsTest" method="assertNumberOfTokensInPlace" />
