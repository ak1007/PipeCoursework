    <action args="T0,(18%2c19)" class="pipe.dataLayer.Transition" method="actionClick" />
    <action args="T0,(19%2c19)" class="pipe.dataLayer.Transition" method="actionClick" />
    <action args="GuiView Instance,(300%2c188),BUTTON1_MASK|SHIFT_MASK" class="pipe.gui.GuiView" method="actionClick" />
