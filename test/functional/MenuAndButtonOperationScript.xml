    <action args="Immediate transition" method="actionSelectMenuItem" />
    <assert component="Images%5cImmediate%20transition.png" method="isSelected" />
    <!-- Click on drawing area and confirm we now have a transition -->
    <action args="GuiView Instance,(247%2c122)" class="pipe.gui.GuiView" method="actionClick" />
    <assert component="T0" method="isVisible" />
    <!-- Click on  Timed Transition button to change mode again and add one of those-->
